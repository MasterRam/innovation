  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.3.7",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "mongoose": "^5.4.4",
    "nodemon": "^1.18.9"
  }
}
